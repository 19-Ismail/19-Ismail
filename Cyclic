#include <iostream>
#include <vector>
using namespace std;
  static bool cycle(int start, int parent,int vis[],vector<int>adj[]){
      vis[start]=1;
      for(int i:adj[start]){
        if(!vis[i]){
            if(cycle(i,start,vis,adj)==true){
                return true;
            }
            else if(i!=parent){
                return true;
            }
        }
      }
    return false;
    }
int main()

{  int m,n,u,v;
   cout<<"Enter the Number of Vertices:";
   cin>>m;
   cout<<"Enter the Number of Edges:";
   cin>>n;
   vector<int>adj[m+1];
   for(int i=0;i<n;i++){
    cout<<"Enter the Connections :";
    cin>>u;
    cin>>v;
    adj[u].push_back(v);
    adj[v].push_back(u);
   }
   for(int i=1;i<=m;i++){
     for(int j:adj[i]){
        cout<<j;
     }
     cout<<"\n";
   }
   int start=1;
   int parent=-1;
   int vis[m+1]={0};
   cout<<cycle(start,parent,vis,adj);


}
