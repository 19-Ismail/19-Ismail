#include <iostream>
#include <vector>
using namespace std;
int main()
{
    int n,mi;
    cout<<"Enter the Total Number of Matrixes:";
    cin>>n;
    n=n+1;
    int a[n];
    for(int i=0;i<n;i++){
        cout<<"Enter the Elements:";
        cin>>a[i];
    }
    int m[n][n];
    int s[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            m[i][j]=0;
            s[i][j]=0;
        }
    }
    for(int d=1;d<n-1;d++){
        for(int i=1;i<n-d;i++){
            int j=i+d;
            mi=32767;
            for(int k=i;k<j;k++){
               int  q=m[i][k]+m[k+1][j]+a[i-1]*a[k]*a[j];
                if(q<mi){
                    mi=q;
                    s[i][j]=k;
                }
            }
            m[i][j]=mi;
        }
    }
    cout<<m[1][n-1];
 }
