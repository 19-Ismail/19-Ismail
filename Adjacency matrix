#include <iostream>
#include <vector>
using namespace std;
  int main(){
      int m,n,u,v;
      cout<<"Enter the Number of vertices:";
      cin>>m;
      cout<<"Enter the Number of Edges:";
      cin>>n;
     int a[m+1][m+1];
     for(int i=0;i<=m;i++){
        for(int j=0;j<=m;j++){
           a[i][j]=0;
        }
     }
     for(int i=0;i<n;i++){
        cout<<"Enter the Connected Edges:";
        cin>>u;
        cin>>v;
        a[u][v]=1;
        a[v][u]=1;
     }
      for(int i=0;i<=m;i++){
        for(int j=0;j<=m;j++){
           cout<<a[i][j];
        }
        cout<<"\n";
     }

}
