#include <iostream>
#import <vector>

using namespace std;
 static void selectionSort(int index[],int start[],int ending[],int n){
           for(int i=0;i<n;i++){
            int minn=ending[i];
            int minIndex=i;
            for(int j=i+1;j<n;j++){
                if(ending[j]<minn){
                    minn=ending[j];
                    minIndex=j;
                }
            }
            int temp=index[i];
            index[i]=index[minIndex];
            index[minIndex]=temp;
            int temp1=start[i];
            start[i]=start[minIndex];
            start[minIndex]=temp1;
            int temp2=ending[i];
            ending[i]=ending[minIndex];
            ending[minIndex]=temp2;
           }


 }
int main()
{   int n,u,v;
    cout<<"Enter the Number of Activities are Going to Happen:";
    cin>>n;
    int index[n];
    for(int i=0;i<n;i++){
        index[i]=i;
    }
    int start[n];
    int ending[n];
    for(int i=0;i<n;i++){
        cout<<"Enter the Starting Time:";
        cin>>u;
        cout<<"Enter the Ending Time:";
        cin>>v;
        start[i]=u;
        ending[i]=v;
    }
    selectionSort(index,start,ending,n);
    cout<<"The Selected Activites are :";
    cout<<index[0];
    int i=0;
    for(int j=1;j<n-1;j++){
        if(start[j]>=ending[i]){
            cout<<index[j];
            i=i+1;
        }
    }
}
